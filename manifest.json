{
 "manifest_version": 2,
 "name": "OKcookies",
 "description": "Never see another privacy message again.",
  "version": "0.002",
  "permissions": [
    "activeTab"
  ],
"browser_action": {
   "default_icon": "tab-icon.png",
   "default_title": "OK cookies!"
 },

"content_scripts": [
	{
	  "matches": [
		"<all_urls>"
	  ],
	  "js": ["content.js"]
	}
	]
}

